@model ProductListViewModel

<ul class="thumbnails">
    @foreach (var item in Model.ListValue)
    {    
        <li class="span4">
            <a href="~/product/details/@item.Id" target="_blank" class="thumbnail text-center">
                <img alt="@item.Name" src="@item.PictureUrl">
                <h5>@item.Name</h5>
            </a>
        </li>
    }
</ul>